{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/combate/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/la-porra/?$",
			"dest": "_render"
		},
		{
			"src": "^/luchador/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}